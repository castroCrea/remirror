{
  "name": "@remirror/messages",
  "version": "0.0.0",
  "private": true,
  "description": "Internalization strings which are used throughout the remirror core codebase.",
  "keywords": [
    "remirror"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/@remirror/messages",
  "repository": "https://github.com/remirror/remirror/tree/HEAD/packages/@remirror/messages",
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/messages.esm.js",
      "require": "./dist/messages.cjs.js",
      "browser": "./dist/messages.esm.js",
      "types": "./dist/messages.cjs.d.ts",
      "default": "./dist/messages.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "dist/remirror-messages.cjs.js",
  "module": "dist/remirror-messages.esm.js",
  "browser": {
    "./dist/remirror-messages.cjs.js": "./dist/remirror-messages.browser.cjs.js",
    "./dist/remirror-messages.esm.js": "./dist/remirror-messages.browser.esm.js"
  },
  "types": "dist/remirror-messages.cjs.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@lingui/core": "^3.3.0",
    "@remirror/core-helpers": "^1.0.0-next.58"
  },
  "publishConfig": {
    "access": "public"
  },
  "meta": {
    "sizeLimit": "50 KB"
  }
}
