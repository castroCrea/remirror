{
  "name": "@remirror/dev",
  "version": "1.0.0-next.58",
  "description": "Development utilities for remirror",
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/@remirror/dev",
  "repository": "https://github.com/remirror/remirror/tree/HEAD/packages/@remirror/dev",
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/remirror-dev.esm.js",
      "require": "./dist/remirror-dev.cjs.js",
      "browser": "./dist/remirror-dev.browser.esm.js",
      "types": "./dist/remirror-dev.cjs.d.ts",
      "default": "./dist/remirror-dev.esm.js"
    },
    "./package.json": "./package.json",
    "./render": {
      "import": "./render/dist/remirror-dev-render.esm.js",
      "require": "./render/dist/remirror-dev-render.cjs.js",
      "browser": "./render/dist/remirror-dev.browser.esm.js",
      "default": "./render/dist/remirror-dev-render.esm.js"
    },
    "./types/*": "./dist/declarations/src/*.d.ts",
    "./worker.json-diff": {
      "import": "./worker.json-diff/dist/remirror-dev-worker.json-diff.esm.js",
      "require": "./worker.json-diff/dist/remirror-dev-worker.json-diff.cjs.js",
      "browser": "./worker.json-diff/dist/remirror-dev.browser.esm.js",
      "default": "./worker.json-diff/dist/remirror-dev-worker.json-diff.esm.js"
    }
  },
  "main": "dist/remirror-dev.cjs.js",
  "module": "dist/remirror-dev.esm.js",
  "browser": {
    "./dist/remirror-dev.cjs.js": "./dist/remirror-dev.browser.cjs.js",
    "./dist/remirror-dev.esm.js": "./dist/remirror-dev.browser.esm.js"
  },
  "types": "dist/remirror-dev.cjs.d.ts",
  "files": [
    "dist",
    "render",
    "worker.json-diff"
  ],
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@emotion/react": "^11.1.2",
    "@emotion/styled": "^11.0.0",
    "@lingui/core": "^3.3.0",
    "@remirror/core": "1.0.0-next.58",
    "@remirror/pm": "1.0.0-next.58",
    "@remirror/react": "1.0.0-next.58",
    "@remirror/react-components": "1.0.0-next.38",
    "@types/html": "^1.0.0",
    "@types/prosemirror-dev-tools": "^2.1.0",
    "create-context-state": "0.0.0",
    "date-fns": "^2.16.1",
    "html": "^1.0.0",
    "idb-keyval": "^3.2.0",
    "jsondiffpatch": "^0.4.1",
    "nanoid": "^3.1.20",
    "promise-worker": "^2.0.1",
    "prosemirror-dev-tools": "^3.0.2",
    "react-dock": "^0.3.0",
    "react-json-tree": "^0.13.0",
    "reakit": "^1.3.2",
    "type-fest": "^0.20.2"
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "peerDependencies": {
    "@types/react": "^16.14.0 || ^17",
    "@types/react-dom": "^16.9.0 || ^17",
    "react": "^16.14.0 || ^17",
    "react-dom": "^16.14.0 || ^17"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "cjs": "lib/dist/dev.cjs.js",
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "render.tsx",
      "worker.json-diff.ts"
    ]
  }
}
