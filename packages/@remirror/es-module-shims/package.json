{
  "name": "@remirror/es-module-shims",
  "version": "0.0.0",
  "private": true,
  "description": "A fork of es-module-shims to support the remirror playground.",
  "keywords": [
    "remirror"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/@remirror/es-module-shims",
  "repository": "https://github.com/remirror/remirror/tree/HEAD/packages/@remirror/es-module-shims",
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/es-module-shims.esm.js",
      "require": "./dist/es-module-shims.cjs.js",
      "browser": "./dist/es-module-shims.esm.js",
      "types": "./dist/es-module-shims.cjs.d.ts",
      "default": "./dist/es-module-shims.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "dist/remirror-es-module-shims.cjs.js",
  "umd:main": "dist/remirror-es-module-shims.umd.min.js",
  "module": "dist/remirror-es-module-shims.esm.js",
  "browser": {
    "./dist/remirror-es-module-shims.cjs.js": "./dist/remirror-es-module-shims.browser.cjs.js",
    "./dist/remirror-es-module-shims.esm.js": "./dist/remirror-es-module-shims.browser.esm.js"
  },
  "types": "dist/remirror-es-module-shims.cjs.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@remirror/core-helpers": "^1.0.0-next.58",
    "@types/es-module-lexer": "^0.3.0",
    "es-module-lexer": "^0.3.26"
  },
  "publishConfig": {
    "access": "public"
  },
  "meta": {
    "sizeLimit": "50 KB"
  },
  "preconstruct": {
    "umdName": "esModuleShims"
  }
}
